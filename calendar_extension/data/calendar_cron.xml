<?xml version="1.0"?>
<openerp>
    <data>
        <!-- Scheduler for Event Alarm-->
        <record forcecreate="True" id="ir_cron_custom_scheduler_alarm" model="ir.cron">
            <field name="name">Run Custom Event Reminder</field>
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
            <field eval="'calendar.alarm_manager'" name="model" />
            <field eval="'get_next_alarm'" name="function" />
            <!--<field eval="'(False,)'" name="args" />-->
        </record>
    </data>
</openerp>